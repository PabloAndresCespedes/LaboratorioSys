create table DEVELOPER.RT_FORMAS_PAGO
(
  id              NUMBER generated by default on null as identity,
  empresa_id      NUMBER,
  descripcion     VARCHAR2(50 CHAR),
  ind_comprobante VARCHAR2(2 CHAR),
  creado          DATE not null,
  creado_por      VARCHAR2(255 CHAR) not null,
  modificado      DATE not null,
  modificado_por  VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_FORMAS_PAGO_I1 on DEVELOPER.RT_FORMAS_PAGO (EMPRESA_ID);
alter table DEVELOPER.RT_FORMAS_PAGO
  add constraint RT_FORMAS_PAGO_ID_PK primary key (ID);
alter table DEVELOPER.RT_FORMAS_PAGO
  add constraint RT_FORMAS_PAGO_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


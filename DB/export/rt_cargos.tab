create table DEVELOPER.RT_CARGOS
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  descripcion    VARCHAR2(50 CHAR),
  abreviatura    VARCHAR2(5 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_CARGOS_I1 on DEVELOPER.RT_CARGOS (EMPRESA_ID);
alter table DEVELOPER.RT_CARGOS
  add constraint RT_CARGOS_ID_PK primary key (ID);
alter table DEVELOPER.RT_CARGOS
  add constraint RT_CARGOS_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


create table DEVELOPER.RT_ESPECIALISTAS
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  nombre         VARCHAR2(255 CHAR),
  apellido       VARCHAR2(255 CHAR),
  tel            VARCHAR2(50 CHAR),
  mail           VARCHAR2(255 CHAR),
  documento      NUMBER,
  registro       VARCHAR2(25 CHAR),
  cargos_ids     VARCHAR2(4000 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_ESPECIALISTAS_I1 on DEVELOPER.RT_ESPECIALISTAS (EMPRESA_ID);
alter table DEVELOPER.RT_ESPECIALISTAS
  add constraint RT_ESPECIALISTAS_ID_PK primary key (ID);
alter table DEVELOPER.RT_ESPECIALISTAS
  add constraint RT_ESPECIALISTA_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


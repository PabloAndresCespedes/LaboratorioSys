create table DEVELOPER.RT_PARAMETROS_USUARIOS
(
  id               NUMBER generated by default on null as identity,
  empresa_id       NUMBER,
  parametro_doc_id NUMBER,
  usuario          VARCHAR2(50 CHAR),
  creado           DATE not null,
  creado_por       VARCHAR2(255 CHAR) not null,
  modificado       DATE not null,
  modificado_por   VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_PARAMETROS_USUA_I1 on DEVELOPER.RT_PARAMETROS_USUARIOS (EMPRESA_ID);
alter table DEVELOPER.RT_PARAMETROS_USUARIOS
  add constraint RT_PARAMETROS_USUA_ID_PK primary key (ID);
alter table DEVELOPER.RT_PARAMETROS_USUARIOS
  add constraint RT_PARAMETROS_U_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


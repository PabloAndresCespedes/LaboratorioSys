create table DEVELOPER.RT_DOCUMENTOS
(
  id                NUMBER generated by default on null as identity,
  empresa_id        NUMBER,
  reserva_id        NUMBER,
  tipo_documento_id NUMBER,
  fecha_emision     DATE,
  establecimiento   NUMBER,
  punto_emision     NUMBER,
  nro               NUMBER,
  nro_completo      VARCHAR2(25 CHAR),
  condicion         VARCHAR2(25 CHAR),
  razon_social      VARCHAR2(4000 CHAR),
  ruc               VARCHAR2(25 CHAR),
  venc              VARCHAR2(4000 CHAR),
  dir               VARCHAR2(4000 CHAR),
  tel               VARCHAR2(25 CHAR),
  creado            DATE not null,
  creado_por        VARCHAR2(255 CHAR) not null,
  modificado        DATE not null,
  modificado_por    VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_DOCUMENTOS_I1 on DEVELOPER.RT_DOCUMENTOS (EMPRESA_ID);
create index DEVELOPER.RT_DOCUMENTOS_I982 on DEVELOPER.RT_DOCUMENTOS (RESERVA_ID);
alter table DEVELOPER.RT_DOCUMENTOS
  add constraint RT_DOCUMENTOS_ID_PK primary key (ID);
alter table DEVELOPER.RT_DOCUMENTOS
  add constraint RT_DOCUMENTOS_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;
alter table DEVELOPER.RT_DOCUMENTOS
  add constraint RT_DOCUMENTOS_RESERVA_ID_FK foreign key (RESERVA_ID)
  references DEVELOPER.RT_RESERVAS (ID) on delete cascade;


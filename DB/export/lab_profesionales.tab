create table DEVELOPER.LAB_PROFESIONALES
(
  id                  NUMBER generated by default on null as identity,
  empresa_id          NUMBER not null,
  ci                  VARCHAR2(25 CHAR),
  ruc                 VARCHAR2(25 CHAR),
  registro            VARCHAR2(25 CHAR),
  nombre              VARCHAR2(200 CHAR),
  apellido            VARCHAR2(200 CHAR),
  tipo_profesional_id NUMBER,
  estado              NUMBER,
  creado              DATE not null,
  creado_por          VARCHAR2(255 CHAR) not null,
  modificado          DATE not null,
  modificado_por      VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.LAB_PROFESIONALES_I1 on DEVELOPER.LAB_PROFESIONALES (TIPO_PROFESIONAL_ID);
alter table DEVELOPER.LAB_PROFESIONALES
  add constraint LAB_PROFESIONALES_ID_PK primary key (ID);
alter table DEVELOPER.LAB_PROFESIONALES
  add constraint LAB_PROFESION_TIPO_PROFESIO_FK foreign key (TIPO_PROFESIONAL_ID)
  references DEVELOPER.LAB_TIPOS_PROFESIONALES (ID) on delete cascade;
alter table DEVELOPER.LAB_PROFESIONALES
  add constraint LAB_PROFESIONALES_ESTADO_CK
  check (estado in (1,0));


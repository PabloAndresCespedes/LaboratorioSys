create table DEVELOPER.RT_RESERVAS
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  paciente_id    NUMBER,
  fecha          DATE,
  turno_id       NUMBER,
  estado         VARCHAR2(25 CHAR),
  obs            VARCHAR2(4000 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_RESERVAS_I1 on DEVELOPER.RT_RESERVAS (EMPRESA_ID);
create index DEVELOPER.RT_RESERVAS_I292 on DEVELOPER.RT_RESERVAS (PACIENTE_ID);
alter table DEVELOPER.RT_RESERVAS
  add constraint RT_RESERVAS_ID_PK primary key (ID);
alter table DEVELOPER.RT_RESERVAS
  add constraint RT_RESERVAS_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;
alter table DEVELOPER.RT_RESERVAS
  add constraint RT_RESERVAS_PACIENTE_ID_FK foreign key (PACIENTE_ID)
  references DEVELOPER.RT_PACIENTES (ID) on delete cascade;


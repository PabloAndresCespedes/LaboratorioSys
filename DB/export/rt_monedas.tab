create table DEVELOPER.RT_MONEDAS
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  descripcion    VARCHAR2(50 CHAR),
  iso            VARCHAR2(5 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_MONEDAS_I1 on DEVELOPER.RT_MONEDAS (EMPRESA_ID);
alter table DEVELOPER.RT_MONEDAS
  add constraint RT_MONEDAS_ID_PK primary key (ID);
alter table DEVELOPER.RT_MONEDAS
  add constraint RT_MONEDAS_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


create table DEVELOPER.RT_TURNOS_DET
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  turno_id       NUMBER,
  desde          DATE,
  hasta          DATE,
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_TURNOS_DET_I1 on DEVELOPER.RT_TURNOS_DET (EMPRESA_ID);
alter table DEVELOPER.RT_TURNOS_DET
  add constraint RT_TURNOS_DET_ID_PK primary key (ID);
alter table DEVELOPER.RT_TURNOS_DET
  add constraint RT_TURNOS_DET_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


create table DEVELOPER.ROLES
(
  id          NUMBER generated by default on null as identity,
  descripcion VARCHAR2(100 CHAR) not null,
  explicacion VARCHAR2(255 CHAR),
  permiso_ids VARCHAR2(4000 CHAR),
  empresa_id  NUMBER not null
)
;
alter table DEVELOPER.ROLES
  add constraint ROLES_ID_PK primary key (ID);
alter table DEVELOPER.ROLES
  add constraint ROLES_DESC_UK unique (DESCRIPCION);


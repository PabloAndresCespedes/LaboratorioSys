create table DEVELOPER.RT_DOCUMENTOS_DET
(
  id              NUMBER generated by default on null as identity,
  empresa_id      NUMBER,
  documento_id    NUMBER,
  item            NUMBER,
  cant            NUMBER,
  descripcion     VARCHAR2(4000 CHAR),
  precio_unitario NUMBER(18,2),
  exentas         NUMBER(18,2),
  impuesto_cinco  NUMBER(18,2),
  impuesto_diez   NUMBER(18,2),
  impu_cinco_calc NUMBER(18,2),
  impu_diez_calc  NUMBER(18,2),
  creado          DATE not null,
  creado_por      VARCHAR2(255 CHAR) not null,
  modificado      DATE not null,
  modificado_por  VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_DOCUMENTOS_DET_I1 on DEVELOPER.RT_DOCUMENTOS_DET (DOCUMENTO_ID);
create index DEVELOPER.RT_DOCUMENTOS_DET_I1032 on DEVELOPER.RT_DOCUMENTOS_DET (EMPRESA_ID);
alter table DEVELOPER.RT_DOCUMENTOS_DET
  add constraint RT_DOCUMENTOS_DET_ID_PK primary key (ID);
alter table DEVELOPER.RT_DOCUMENTOS_DET
  add constraint RT_DOCUMENTOS_D_DOCUMENTO_I_FK foreign key (DOCUMENTO_ID)
  references DEVELOPER.RT_DOCUMENTOS (ID) on delete cascade;
alter table DEVELOPER.RT_DOCUMENTOS_DET
  add constraint RT_DOCUMENTOS_D_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


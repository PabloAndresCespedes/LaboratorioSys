create table DEVELOPER.RT_SERVICIOS
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  descripcion    VARCHAR2(4000 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_SERVICIOS_I1 on DEVELOPER.RT_SERVICIOS (EMPRESA_ID);
alter table DEVELOPER.RT_SERVICIOS
  add constraint RT_SERVICIOS_ID_PK primary key (ID);
alter table DEVELOPER.RT_SERVICIOS
  add constraint RT_SERVICIOS_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


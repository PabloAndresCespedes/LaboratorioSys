create table DEVELOPER.USUARIOS
(
  id          NUMBER generated by default on null as identity,
  nombre      VARCHAR2(100 CHAR) not null,
  apellido    VARCHAR2(100 CHAR) not null,
  login       VARCHAR2(10 CHAR) not null,
  pass        VARCHAR2(100 CHAR) not null,
  codigo      NUMBER not null,
  activo      VARCHAR2(1 CHAR) default 'S',
  rol_ids     VARCHAR2(4000 CHAR),
  empresa_ids VARCHAR2(4000 CHAR)
)
;
alter table DEVELOPER.USUARIOS
  add constraint USUARIOS_ID_PK primary key (ID);
alter table DEVELOPER.USUARIOS
  add constraint USUARIO_ACTIVO_CK
  check (ACTIVO IN ('S', 'N'));


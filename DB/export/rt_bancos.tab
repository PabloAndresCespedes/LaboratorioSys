create table DEVELOPER.RT_BANCOS
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  descripcon     VARCHAR2(50 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_BANCOS_I1 on DEVELOPER.RT_BANCOS (EMPRESA_ID);
alter table DEVELOPER.RT_BANCOS
  add constraint RT_BANCOS_ID_PK primary key (ID);
alter table DEVELOPER.RT_BANCOS
  add constraint RT_BANCOS_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


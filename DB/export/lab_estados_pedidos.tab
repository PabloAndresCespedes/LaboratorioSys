create table DEVELOPER.LAB_ESTADOS_PEDIDOS
(
  id             NUMBER generated by default on null as identity,
  codigo         NUMBER not null,
  descripcion    VARCHAR2(25 CHAR) not null,
  explicacion    VARCHAR2(200 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
alter table DEVELOPER.LAB_ESTADOS_PEDIDOS
  add constraint LAB_ESTADOS_PEDIDO_ID_PK primary key (ID);


;
create index DEVELOPER.LAB_ESTUDIOS_PRECI_I1 on DEVELOPER.LAB_ESTUDIOS_PRECIOS (ESTUDIO_ID);
alter table DEVELOPER.LAB_ESTUDIOS_PRECIOS
  add constraint LAB_ESTUDIOS_PRECI_ID_PK primary key (ID);
alter table DEVELOPER.LAB_ESTUDIOS_PRECIOS
  add constraint LAB_ESTUDIOS_PR_ESTUDIO_ID_FK foreign key (ESTUDIO_ID)

create table DEVELOPER.RT_PACIENTES
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  nombre         VARCHAR2(255 CHAR),
  apellido       VARCHAR2(255 CHAR),
  documento      NUMBER,
  fecha_nac      DATE,
  tel            VARCHAR2(50 CHAR),
  mail           VARCHAR2(255 CHAR),
  dir            VARCHAR2(4000 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_PACIENTES_I1 on DEVELOPER.RT_PACIENTES (EMPRESA_ID);
alter table DEVELOPER.RT_PACIENTES
  add constraint RT_PACIENTES_ID_PK primary key (ID);
alter table DEVELOPER.RT_PACIENTES
  add constraint RT_PACIENTES_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


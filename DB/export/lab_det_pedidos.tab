create table DEVELOPER.LAB_DET_PEDIDOS
(
  id                      NUMBER generated by default on null as identity,
  pedido_id               NUMBER,
  item                    NUMBER not null,
  valor_referencia_id     NUMBER,
  valor_estudio           VARCHAR2(4000 CHAR),
  monto_bruto             NUMBER(20,2),
  monto_impuesto          NUMBER(20,2),
  monto_gravado           NUMBER(20,2),
  impuesto_porc           NUMBER(20,2),
  impuesto_base_imponible NUMBER(20,2),
  creado                  DATE not null,
  creado_por              VARCHAR2(255 CHAR) not null,
  modificado              DATE not null,
  modificado_por          VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.LAB_DET_PEDIDOS_I1 on DEVELOPER.LAB_DET_PEDIDOS (PEDIDO_ID);
create index DEVELOPER.LAB_DET_PEDIDOS_I462 on DEVELOPER.LAB_DET_PEDIDOS (VALOR_REFERENCIA_ID);
alter table DEVELOPER.LAB_DET_PEDIDOS
  add constraint LAB_DET_PEDIDOS_ID_PK primary key (ID);
alter table DEVELOPER.LAB_DET_PEDIDOS
  add constraint LAB_DET_PEDIDOS_PEDIDO_ID_FK foreign key (PEDIDO_ID)
  references DEVELOPER.LAB_PEDIDOS (ID) on delete cascade;
alter table DEVELOPER.LAB_DET_PEDIDOS
  add constraint LAB_DET_PEDID_VALOR_REFEREN_FK foreign key (VALOR_REFERENCIA_ID)
  references DEVELOPER.LAB_VALORES_REFERENCIA (ID) on delete cascade;


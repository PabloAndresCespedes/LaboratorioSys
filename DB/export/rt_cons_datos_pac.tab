create table DEVELOPER.RT_CONS_DATOS_PAC
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  consulta_id    NUMBER,
  nombre         VARCHAR2(255 CHAR),
  apellido       VARCHAR2(255 CHAR),
  documento      NUMBER,
  fecha_nac      DATE,
  edad           VARCHAR2(255 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_CONS_DATOS_PAC_I1 on DEVELOPER.RT_CONS_DATOS_PAC (CONSULTA_ID);
create index DEVELOPER.RT_CONS_DATOS_PAC_I682 on DEVELOPER.RT_CONS_DATOS_PAC (EMPRESA_ID);
alter table DEVELOPER.RT_CONS_DATOS_PAC
  add constraint RT_CONS_DATOS_PAC_ID_PK primary key (ID);
alter table DEVELOPER.RT_CONS_DATOS_PAC
  add constraint RT_CONS_DATOS_P_CONSULTA_ID_FK foreign key (CONSULTA_ID)
  references DEVELOPER.RT_CONSULTAS (ID) on delete cascade;
alter table DEVELOPER.RT_CONS_DATOS_PAC
  add constraint RT_CONS_DATOS_P_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;


create table DEVELOPER.RT_TIPOS_DOCUMENTOS
(
  id             NUMBER generated by default on null as identity,
  descripcion    VARCHAR2(255 CHAR),
  abreviatura    VARCHAR2(10 CHAR),
  tipo           VARCHAR2(2 CHAR),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
alter table DEVELOPER.RT_TIPOS_DOCUMENTOS
  add constraint RT_TIPOS_DOCUMENTO_ID_PK primary key (ID);


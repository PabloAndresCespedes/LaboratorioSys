create table DEVELOPER.RT_COTIZACIONES
(
  id             NUMBER generated by default on null as identity,
  empresa_id     NUMBER,
  moneda_id      NUMBER,
  fecha          NUMBER(18,2),
  compra         NUMBER(18,2),
  venta          NUMBER(18,2),
  creado         DATE not null,
  creado_por     VARCHAR2(255 CHAR) not null,
  modificado     DATE not null,
  modificado_por VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_COTIZACIONES_I1 on DEVELOPER.RT_COTIZACIONES (EMPRESA_ID);
create index DEVELOPER.RT_COTIZACIONES_I792 on DEVELOPER.RT_COTIZACIONES (MONEDA_ID);
alter table DEVELOPER.RT_COTIZACIONES
  add constraint RT_COTIZACIONES_ID_PK primary key (ID);
alter table DEVELOPER.RT_COTIZACIONES
  add constraint RT_COTIZACIONES_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;
alter table DEVELOPER.RT_COTIZACIONES
  add constraint RT_COTIZACIONES_MONEDA_ID_FK foreign key (MONEDA_ID)
  references DEVELOPER.RT_MONEDAS (ID) on delete cascade;


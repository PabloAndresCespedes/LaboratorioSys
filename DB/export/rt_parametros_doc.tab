create table DEVELOPER.RT_PARAMETROS_DOC
(
  id              NUMBER generated by default on null as identity,
  empresa_id      NUMBER,
  establecimiento VARCHAR2(25 CHAR),
  punto_emision   VARCHAR2(25 CHAR),
  timb_nro        NUMBER,
  timb_inicio     DATE,
  timb_fin        DATE,
  nro_inicio      NUMBER,
  nro_fin         NUMBER,
  creado          DATE not null,
  creado_por      VARCHAR2(255 CHAR) not null,
  modificado      DATE not null,
  modificado_por  VARCHAR2(255 CHAR) not null
)
;
create index DEVELOPER.RT_PARAMETROS_DOC_I1 on DEVELOPER.RT_PARAMETROS_DOC (EMPRESA_ID);
alter table DEVELOPER.RT_PARAMETROS_DOC
  add constraint RT_PARAMETROS_DOC_ID_PK primary key (ID);
alter table DEVELOPER.RT_PARAMETROS_DOC
  add constraint RT_PARAMETROS_D_EMPRESA_ID_FK foreign key (EMPRESA_ID)
  references DEVELOPER.RT_EMPRESAS (ID) on delete cascade;

